!!!
%html
  %head
    %meta{  content: "text/html; charset=UTF-8",
            "http-equiv": "Content-Type"}
    %meta{  name:"viewport",
            content:"width=device-width, initial-scale=1.0"}
    %title OmniBNK Coding Challenge
    = csrf_meta_tags
    = csp_meta_tag
    %link{  href: "https://cdnjs.cloudflare.com/ajax/libs/bulma/0.7.5/css/bulma.min.css",
            type: "text/css",
            rel: "stylesheet" }
    = stylesheet_link_tag     'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_include_tag  'application', 'data-turbolinks-track': 'reload'
  %body
    = yield
    = render "shared/footer"
    %script{  defer: "defer", 
              src: "https://use.fontawesome.com/releases/v5.3.1/js/all.js" }
    %script{  src:"//instant.page/1.1.0",
              type:"module",
              integrity:"sha384-EwBObn5QAxP8f09iemwAJljc+sU+eUXeL9vSBw1eNmVarwhKk2F9vBEpaN9rsrtp" }/
    
